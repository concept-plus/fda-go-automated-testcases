{
    "src_folders": ["./examples/tests"],
    "output_folder": "./examples/reports",
    "custom_commands_path": "./examples/custom-commands",
    "custom_assertions_path": "",
    "globals_path": "./examples/globalsModule.js",
    "live_output": false,
    "parallel_process_delay": 10,
    "disable_colors": false,
    "test_workers": false,

    "test_settings": {
        "development": {
            "launch_url": "http://fdago-dev.conceptplusllc.net/#/",
            "live_output": true,
            "silent": true,
            "screenshots": {
                "enabled": true,
                "path": "./examples/reports/",
                "on_failure": true
            },
            "desiredCapabilities": {
                "browserName": "phantomjs",
                "javascriptEnabled": true,
                "acceptSslCerts": true,
                "phantomjs.binary.path": "phantomjs"
            }
        },
        "localmac": {
            "launch_url": "http://fdago-dev.conceptplusllc.net/#/",
            "silent": true,
            "selenium_port": 4444,
            "selenium_host": "localhost",
            "silent": true,
            "screenshots": {
                "enabled": true,
                "path": "./examples/reports/screenshots/",
                "on_failure": true
            },
            "desiredCapabilities": {
                "browserName": "firefox",
                "javascriptEnabled": true,
                "acceptSslCerts": true
            }
        },
        "production": {
            "launch_url": "http://fdago.conceptplusllc.net/#/",
            "live_output": true,
            "silent": true,
            "screenshots": {
                "enabled": true,
                "path": "./examples/reports/",
                "on_failure": true
            },
            "desiredCapabilities": {
                "browserName": "phantomjs",
                "javascriptEnabled": true,
                "acceptSslCerts": true,
                "phantomjs.binary.path": "phantomjs"
            }
        },
        "phantomjs": {
            "desiredCapabilities": {
                "browserName": "phantomjs",
                "javascriptEnabled": true,
                "acceptSslCerts": true,
                "phantomjs.binary.path": "phantomjs.exe"
            }
        }
    }
}